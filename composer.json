{
    "name": "swag/extend-js-plugin",
    "description": "Example plugin to extend an existing javascript storefront plugin.",
    "version": "v1.0.0",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "shopware AG"
        }
    ],
    "require": {
        "shopware/core": "*",
        "shopware/storefront": "*"
    },
    "extra": {
        "shopware-plugin-class": "Swag\\ExtendJsPlugin\\ExtendJsPlugin",
        "label": {
            "de-DE": "Label für das Plugin ExtendJsPlugin",
            "en-GB": "Label for the plugin ExtendJsPlugin"
        },
        "description": {
            "de-DE": "Beschreibung für das Plugin ExtendJsPlugin",
            "en-GB": "Description for the plugin ExtendJsPlugin"
        }
    },
    "autoload": {
        "psr-4": {
            "Swag\\ExtendJsPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\ExtendJsPlugin\\Tests\\": "tests/"
        }
    }
}
